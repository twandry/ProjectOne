
@{
    ViewBag.Title = "MissionType";
}
<!DOCTYPE html>
<head>
    <style>
        table {
            align-content:center;
            vertical-align:central;
            width:auto;
        }
        td{
            padding-left:10px;
            padding-right:10px;
            padding-top:3px;
            padding-bottom: 3px;
           
        }
        th {
            text-align: center;
            vertical-align: central; 
            padding-left:10px;
            padding-right: 10px;
            padding-top: 3px;
            padding-bottom: 3px;
        }
    </style>
</head>
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item">@Html.ActionLink("Home", "Index", "Home")</li>
        <li class="breadcrumb-item">@Html.ActionLink("Mission", "Index", "Mission")</li>
        <li class="breadcrumb-item active" aria-current="page">@ViewBag.Name</li>

    </ol>
</nav>
<center>
    <img src="~/Content/@ViewBag.Pic" width=30% />
</center>

<br />
<br />


@Html.Raw((string)ViewBag.Output)

<br />
<br />


<p></p>
<h1>FAQ's</h1>

<p>@Html.Raw((string)ViewBag.FAQ)</p>

<br />
<br />


<h2>Ask a Question</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(false, "", new { @class = "text-danger" })


    <form action="">
        Submit Your Question:<br>
        <textarea rows="4" cols="250">
    </textarea>
    </form>

    <input type="submit" value="Submit Registration" />
}

